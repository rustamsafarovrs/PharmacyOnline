<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>
        <variable
            name="lastMedicineViewModel"
            type="tj.rs.pharmacyonline.ui.lastmedicine.LastMedicineViewModel" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <include android:id="@+id/includeLastMedicine"
            layout="@layout/fragment_last_medicine"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:viewModel="@{lastMedicineViewModel}"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>